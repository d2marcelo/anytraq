package com.brtracker.shared.utils.packet;

public class InvalidPacketException extends PacketReaderException {

	private static final long serialVersionUID = 1L;

	public InvalidPacketException() {
	}

	public InvalidPacketException(String message) {
		super(message);
	}

	public InvalidPacketException(Throwable cause) {
		super(cause);
	}

	public InvalidPacketException(String message, Throwable cause) {
		super(message, cause);
	}

	public InvalidPacketException(String message, PacketReaderContext context) {
		super(message, context);
	}

	public InvalidPacketException(String message, Throwable e,
			PacketReaderContext context) {
		super(message, e, context);
	}

	public InvalidPacketException(Throwable e, PacketReaderContext context) {
		super(e, context);
	}

}
