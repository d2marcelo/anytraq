package com.brtracker.shared.utils.math;

public class UnitConversionUtils {

	public static double mphToKph(String mphStr) {
		double mph = Double.valueOf(mphStr);
		return mph*1.60934;
	}
	public static double milesToKm(String milesStr) {
		double miles = Double.valueOf(milesStr);
		return miles*1.60934;
	}

}
